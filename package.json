{
  "name": "pssh-tools",
  "version": "1.0.1",
  "description": "Tools to generate PSSH Data and PSSH Box",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepare": "tsc -p .",
    "build": "tsc -p .",
    "pretest": "tslint -p .",
    "test": "tsc -p .&& cp -R ./src/lib/pssh/proto ./dist/src/lib/pssh/ && ava ./dist/test/*.js --verbose"
  },
  "keywords": [
    "pssh",
    "tools",
    "pssh-box",
    "pssh-data",
    "cenc",
    "drm"
  ],
  "author": "Indra Gunawan <feedsbrain@gmail.com>",
  "license": "BSD",
  "dependencies": {
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.2"
  },
  "files": [
    "dist/src/*",
    "dist/src/**/*"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}